{
    "name": "sebble/flarum-ext-auth-gitlab",
    "description": "Allow users to log in with GitLab.",
    "type": "flarum-extension",
    "keywords": ["authentication"],
    "version": "v0.1.0-beta.8.3.7",
    "license": "MIT",
    "authors": [
        {
            "name": "Toby Zerner",
            "email": "toby.zerner@gmail.com"
        },
        {
            "name": "Sebastian Mellor",
            "email": "sebble@sebble.com"
        }
    ],
    "support": {
        "issues": "https://github.com/sebble/flarum-ext-auth-gitlab/issues",
        "source": "https://github.com/sebble/flarum-ext-auth-gitlab"
    },
    "require": {
        "flarum/core": "^0.1.0-beta.6",
        "omines/oauth2-gitlab": "^1.1.0"
    },
    "autoload": {
        "psr-4": {
            "Sebble\\Auth\\GitLab\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "GitLab Login",
            "icon": {
                "name": "gitlab",
                "backgroundColor": "#548",
                "color": "#fff"
            }
        }
    }
}
